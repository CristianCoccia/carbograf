<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0"  id="com.sistemasje.carbograf" version="1.0.0">
  <name>Carbograf C.A</name>
  <description>Venta de productos de carbón y gráfito para Carbograf C.A</description>
  <author href="http://sistemasje.com" email="distritohosting@gmail.com">Sistemas JE C.A</author>
  <content src="index.html"/>
  <!-- access origins-->
  <access origin="maps.google.com" />
  <access origin="*.googleapis.com" />
  <access origin="*.gstatic.com" />
  <access origin="http://127.0.0.1*"/> <!-- allow local pages -->
  <access origin="http://www.server05.distritohosting.com/" />
  <access origin="http://www.server05.distritohosting.com/" subdomains="true" />
  <access origin=".*"/>
  <preference name="loadUrlTimeoutValue" value="700000" />  
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="orientation" value="portrait"/>
  <plugin name="cordova-plugin-whitelist" version="1" />
  <plugin name="cordova-plugin-camera" source="npm"/>
  <plugin name="cordova-plugin-file" source="npm"/>
  <plugin name="cordova-plugin-file-transfer" source="npm"  />
  <icon src="icon.png"/>
  <platform name="android">
    <icon src="www/icon.png">
  </platform>
  <platform name="ios">
 	<icon src="www/icon.png">
  </platform>
  <platform name="wp8">
		<icon src="www/icon.png">
  </platform>
  <platform name="windows">
 	<icon src="www/icon.png">   
  </platform>
  <plugin name="cordova-plugin-camera" source="npm"  />
    <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <access origin="maps.google.com" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>